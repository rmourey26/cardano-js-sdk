{
  "name": "@cardano-sdk/cardano-graphql-services",
  "version": "0.2.0",
  "description": "Cardano GraphQL Services",
  "engines": {
    "node": "^14"
  },
  "main": "dist/index.js",
  "bin": {
    "dgraph-projector": "./dist/DataProjection/cli.js"
  },
  "repository": "https://github.com/input-output-hk/cardano-js-sdk/packages/cardano-graphql-services",
  "author": "Rhys Bartels-Waller <rhys.bartelswallet@iohk.io>",
  "license": "MPL-2.0",
  "scripts": {
    "build": "tsc --build ./src",
    "tscNoEmit": "shx echo typescript --noEmit command not implemented yet",
    "cleanup": "shx rm -rf dist node_modules",
    "lint": "eslint --ignore-path ../../.eslintignore \"**/*.ts\"",
    "lint:fix": "eslint --fix --ignore-path ../../.eslintignore \"**/*.ts\"",
    "test": "jest -c ./jest.config.js",
    "test:e2e": "shx echo 'test:e2e' command not implemented yet",
    "coverage": "yarn test --coverage",
    "prepack": "yarn build",
    "test:debug": "DEBUG=true yarn test",
    "cli:dgraph-projector": "ts-node --transpile-only src/DataProjection/cli.ts",
    "run:dgraph-projector": "ts-node --transpile-only src/DataProjection/run.ts"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/death": "^1.1.2",
    "@types/google-protobuf": "^3.15.5",
    "dgraph-js": "^21.3.1",
    "shx": "^0.3.3",
    "ts-node": "^10.7.0"
  },
  "dependencies": {
    "@cardano-ogmios/client": "~5.1.0",
    "@cardano-sdk/core": "0.2.0",
    "bunyan": "^1.8.15",
    "clear": "^0.1.0",
    "commander": "^9.0.0",
    "death": "^1.1.0",
    "ts-log": "^2.2.4"
  },
  "files": [
    "dist/*",
    "!dist/tsconfig.tsbuildinfo",
    "LICENSE",
    "NOTICE"
  ]
}
